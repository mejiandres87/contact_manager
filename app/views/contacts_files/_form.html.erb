<div class="container">
  <h1> Import a new file </h1>
  <%= form_with(model: @contacts_file, url: url_for(controller: 'contacts_files', action: 'create'), local: true) do |form| %>
      <div class="form-group">
        <div class="custom-file">
          <%= form.file_field :csv_file, class:"custom-file-input" %>
          <%= form.label :csv_file, class:"custom-file-label" %>
        </div>
      </div>
      <div class="form-group row justify-content-center">
        <%= form.submit class: "btn btn-primary" %>
      </div>
  <% end %>
</div>